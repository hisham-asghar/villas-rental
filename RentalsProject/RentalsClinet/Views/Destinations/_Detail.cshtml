                },
                { "featureType": "road", "elementType": "labels.text.fill", "stylers": [{ "color": "#767676" }] },
                { "featureType": "road", "elementType": "labels.text.stroke", "stylers": [{ "color": "#ffffff" }] },
                { "featureType": "poi", "stylers": [{ "visibility": "off" }] },
                {
