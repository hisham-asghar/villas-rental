<body class="section-white" @(ViewBag.load ?? "")  style="overflow-x:hidden;">
@RenderBody()
<div class="clearfix"></div>
<footer class="footer footer-black footer-big">
    <div class="container">
        <div class="content">
            <div class="row services-list">
                @foreach(var s in ViewBag.services)
                {
                    <div class="col-md-1 col-md-offset-1">
                        <a href="/Services/Detail/@s.guid" class="">
                            <img src="@ViewBag.UploadLoca/@s.ImageSmall" alt="Thumbnail Image" class="img-circle img-raised img-responsive">
                        </a>
                    </div>   
                }
            </div>

            <hr/>
            <div class="row">
                <div class="col-md-2">
                    <h5>DESTINATIONS</h5>
                    <ul class="links-vertical">
                        @foreach (var c in ViewBag.cities)
                        {
                            <li><a href="/Destinations?token=@c.guid">@c.CityName</a></li>
                        }
                    </ul>
                </div>
                <div class="col-md-2 col-md-offset-1">
                    <h5>YACHTS</h5>
                    <ul class="links-vertical">
                        <li><a href="/Yachts?token=34-63">34-63 Feet</a></li>
                        <li><a href="/Yachts?token=64-77">64-77 Feet</a></li>
                        <li><a href="/Yachts?token=78-99">78-99 Feet</a></li>
                        <li><a href="/Yachts?token=100+">100+ Feet</a></li>
                    </ul>
                    <h5>CARS</h5>
                    <ul class="links-vertical">
                        <li><a href="/Cars/Luxury">Luxury Cars</a></li>
                        <li><a href="/Cars/Exotic">Exotic Cars</a></li>
                    </ul>
                </div>
                <div class="col-md-2 col-md-offset-1">
                    <h5>USEFUL LINKS</h5>
                    <ul class="links-vertical">
                        <li><a href="/Home/About">ABOUT US</a></li>
                        <li><a href="/Home/Contact/">CONTACT US</a></li>
                        <li><a href="/Home/CONCIERGE">CONCIERGE</a></li>
                        <li><a href="/Magzines">OUR BLOG</a></li>
                        <li><a href="/Home/Testimonals">TESTIMONALS</a></li>
                        <li><a href="/Home/TERMS-OF-SERVICE">TERMS OF SERVICE</a></li>
                        <li><a href="/Home/POLICY">POLICY</a></li>
                        <li><a href="/Home/FAQ">FAQ</a></li>
                        <li><a href="/Services">SERVICES</a></li>
                    </ul>
                </div>
                <div class="col-md-3 col-md-offset-1">
                    <h5>Follow Us</h5>
                    <ul class="social-buttons">
                        <li>
                            <a href="https://twitter.com/TheMarucaGroup" title="Twitter" target="blank" class="btn btn-just-icon btn-simple btn-twitter">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.facebook.com/TheMarucaGroup/" title="Facebook" target="blank" class="btn btn-just-icon btn-simple btn-facebook">
                                <i class="fa fa-facebook-square"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.youtube.com/channel/UCLw70uM-OTE41EHLZCTwodQ" title="YouTube" target="blank" class="btn btn-just-icon btn-simple btn-youtube">
                                <i class="fa fa-youtube-play"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://plus.google.com/108056308587956692719/posts" title="Google+" target="blank" class="btn btn-just-icon btn-simple btn-google">
                                <i class="fa fa-google-plus"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.instagram.com/themarucagroup/" title="Instagram" target="blank" class="btn btn-just-icon btn-simple btn-instagram">
                                <i class="fa fa-instagram"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.linkedin.com/company/the-maruca-group" title="LinkedIn" target="blank" class="btn btn-just-icon btn-simple btn-linkedin">
                                <i class="fa fa-linkedin-square"></i>
                            </a>
                        </li>
                        <li>
                            <a href="https://www.pinterest.com/themarucagroup" title="Pinterest" target="blank" class="btn btn-just-icon btn-simple btn-pinterest">
                                <i class="fa fa-pinterest"></i>
                            </a>
                        </li>
                    </ul>
                    <br/><br/>
                    <div align="center">
                        <span style="color: #ffffff;"><strong>305-218-5216</strong></span><br>
                        <span style="color: #ffffff;"><strong><a href="mailto:reservations@themarucagroup.com" hidefocus="true" style="outline: none;">reservations@themarucagroup.com</a></strong></span>
                    </div>
                </div>
