    <style>
        p,h1,h2,h3,h4,h5,h6 {
            font-family: arial, helvetica, sans-serif !important;
        }
         .services-list img{
             width: 70px !important;
             height: 70px !important;
         }
        .services-list.small-list img {
            margin: 5%;
        }
        .page-header.header-filter{
            background: #595959; /* Old browsers */
            background: -moz-linear-gradient(-45deg, #595959 12%, #4c4c4c 16%, #4c4c4c 16%, #666666 25%, #111111 41%, #2c2c2c 50%, #111111 56%, #474747 79%, #474747 79%, #111111 87%, #111111 87%, #131313 100%); /* FF3.6-15 */
