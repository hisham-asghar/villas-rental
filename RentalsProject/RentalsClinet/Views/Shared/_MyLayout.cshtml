    @{        
        ViewBag.cities = LayerDAO.PropertyDAO.GetCities().Take(10);
        ViewBag.services = LayerDAO.ServicesDao.GetAll();
        ViewBag.UploadLoca = RentalsClinet.Helpers.CustomActions.getUploadLocation();
