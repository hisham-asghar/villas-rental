<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    
    <link rel="apple-touch-icon" sizes="76x76" href="/Content/assets/img/apple-icon.png" />
    
    <link rel="icon" type="image/png" href="/Content/assets/img/favicon.png" />
    
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>Admin | The Maruca Group </title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    
    <meta name="viewport" content="width=device-width" />
    
    <!-- Bootstrap core CSS     -->
    <link href="/Content/assets/css/bootstrap.min.css" rel="stylesheet" />
    
    <!--  Material Dashboard CSS    -->
    <link href="/Content/assets/css/material-dashboard.css" rel="stylesheet" />

    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="/Content/assets/css/demo.css" rel="stylesheet" />
    
    <!--     Fonts and icons     -->
    <link rel="stylesheet"
          href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    
    <link rel="stylesheet" type="text/css" 
          href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Material+Icons" />
</head>

<body>
<div class="wrapper">
    @Html.Partial("__Sidebar")
    <div class="main-panel">
        @Html.Partial("__Header")
        <div class="content">
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
        @Html.Partial("__Footer")
    </div>
</div>

    <!--   Core JS Files   -->
    <script src="/Content/assets/js/jquery-3.1.1.min.js" type="text/javascript"></script>

    <script src="/Content/assets/js/jquery-ui.min.js" type="text/javascript"></script>

    <script src="/Content/assets/js/bootstrap.min.js" type="text/javascript"></script>

    <script src="/Content/assets/js/material.min.js" type="text/javascript"></script>

    <script src="/Content/assets/js/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>

    <!-- Forms Validations Plugin -->
    <script src="/Content/assets/js/jquery.validate.min.js"></script>

    <!--  Plugin for Date Time Picker and Full Calendar Plugin-->
    <script src="/Content/assets/js/moment.min.js"></script>

    <!--  Charts Plugin -->
    <script src="/Content/assets/js/chartist.min.js"></script>

    <!--  Plugin for the Wizard -->
    <script src="/Content/assets/js/jquery.bootstrap-wizard.js"></script>

    <!--  Notifications Plugin    -->
    <script src="/Content/assets/js/bootstrap-notify.js"></script>

    <!-- DateTimePicker Plugin -->
    <script src="/Content/assets/js/bootstrap-datetimepicker.js"></script>

    <!-- Vector Map plugin -->
    <script src="/Content/assets/js/jquery-jvectormap.js"></script>

    <!-- Sliders Plugin -->
    <script src="/Content/assets/js/nouislider.min.js"></script>

    <!--  Google Maps Plugin    -->

    <!-- Select Plugin -->
    <script src="/Content/assets/js/jquery.select-bootstrap.js"></script>

    <!--  DataTables.net Plugin    -->
    <script src="/Content/assets/js/jquery.datatables.js"></script>

    <!--	Plugin for Fileupload, full documentation here:
     http://www.jasny.net/bootstrap/javascript/#fileinput -->
    <script src="/Content/assets/js/jasny-bootstrap.min.js"></script>

    <!--  Full Calendar Plugin    -->
    <script src="/Content/assets/js/fullcalendar.min.js"></script>

    <!-- TagsInput Plugin -->
    <script src="/Content/assets/js/jquery.tagsinput.js"></script>

    <!-- Material Dashboard javascript methods -->
    <script src="/Content/assets/js/material-dashboard.js"></script>

    <!-- Sweet Alert 2 plugin -->
    <script src="/Content/assets/js/sweetalert2.js"></script>

    <!-- Material Dashboard DEMO methods, don't include it in your project! -->
    <script src="/Content/assets/js/demo.js"></script>

    @RenderSection("scripts",false)
    
    <script>
        $(document).ready(function () {
            $("textarea").each(function () {
                $(this).val($(this).val().trim());
            });
        });
    </script>

</body>

</html>